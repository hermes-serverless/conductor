  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hermes-serverless/function-orchestrator/issues"
  },
  "homepage": "https://github.com/hermes-serverless/function-orchestrator#readme",
  "dependencies": {
    "@hermes-serverless/circular-buffer": "^0.1.0",
    "@hermes-serverless/custom-promises": "^0.1.0",
    "@hermes-serverless/fs-utils": "^0.0.2",
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "double-ended-queue": "^2.1.0-0",
    "execa": "^2.0.3",
    "express": "^4.17.0",
    "http-proxy": "^1.17.0",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "signal-exit": "^3.0.2",
    "winston": "^3.2.1",
    "wtfnode": "^0.8.0"
  },
  "devDependencies": {
    "@hermes-serverless/api-types-db-manager": "^0.0.2",
    "@hermes-serverless/api-types-function-watcher": "^0.0.1",
    "@types/double-ended-queue": "^2.1.1",
    "@types/express": "^4.16.1",
    "@types/http-proxy": "^1.17.0",
    "@types/jest": "^24.0.11",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.6.9",
    "@types/ramda": "^0.26.8",
    "@types/redis": "^2.8.12",
    "@types/supertest": "^2.0.8",
    "folder-hash": "^3.2.0",
    "get-port": "^5.0.0",
    "get-stream": "^5.1.0",
    "jest": "^24.7.1",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  }
